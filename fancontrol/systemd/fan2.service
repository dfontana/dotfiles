[Unit]
Description=liquidctl Fan3 (Top) Control
After=liquidcfg.service

[Service]
User=root
ExecStart=/home/koss/dotfiles/fancontrol/liquidctl/extra/yoda --match nzxt control fan2 with '(0,0),(20,0),(30,20),(40,40),(50,60),(70,100)' on nct6793.cputin
Restart=on-failure

[Install]
WantedBy=multi-user.target 
