[Unit]
Description=liquidctl Fan3 (Top) Control
After=liquidcfg.service

[Service]
User=root
ExecStart=/home/koss/dotfiles/fancontrol/liquidctl/extra/yoda --match nzxt control fan3 with '(0,0),(40,0),(50,20),(60,40),(70,60),(80,100)' on nct6793.cputin
Restart=on-failure

[Install]
WantedBy=multi-user.target 
